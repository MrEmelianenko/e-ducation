<div class="page-header">
  <h1><%= t('.title') %></h1>
</div>

<table class="table table-hover">
  <thead>
    <tr>
      <th><%= t('.name') %></th>
    </tr>
  </thead>
  <tbody>
    <% subjects.each do |subject| %>
      <tr>
        <td><%= link_to subject, subject %></td>
      </tr>
    <% end %>
  </tbody>
</table>
