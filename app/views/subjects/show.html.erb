<div class="page-header">
  <h1><%= t('.title', name: subject) %></h1>
</div>

<div class="page-sub-header">
  <h4><%= t('.teachers') %></h4>
</div>

<ul class="clear-list">
  <% subject.users.each do |user| %>
    <li><%= link_to user, user %></li>
  <% end %>
</ul>

<div class="page-sub-header">
  <h4><%= t('.schedule') %></h4>
</div>

<%= render partial: 'schedules/calendar', locals: { schedules: subject.schedules } %>


